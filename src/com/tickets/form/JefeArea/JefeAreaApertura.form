<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.tickets.form.JefeArea.JefeAreaApertura">
  <grid id="27dc6" binding="pnlApertura" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="1b2cd" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="ef730" class="javax.swing.JLabel" binding="lblTitle">
            <constraints border-constraint="North"/>
            <properties>
              <font name="Consolas" size="36" style="1"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Aperturando nuevo caso"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="e055a" layout-manager="GridLayoutManager" row-count="2" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="f2306" class="javax.swing.JLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Título del proyecto: "/>
            </properties>
          </component>
          <component id="4b120" class="javax.swing.JLabel">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Descripción del proyecto"/>
            </properties>
          </component>
          <component id="d7485" class="javax.swing.JTextField" binding="txtName">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties/>
          </component>
          <component id="43915" class="javax.swing.JTextArea" binding="txaDesc">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="50"/>
              </grid>
            </constraints>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="2c1d3" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="c6f33" class="javax.swing.JButton" binding="btnEnviar">
            <constraints/>
            <properties>
              <text value="Enviar"/>
            </properties>
          </component>
          <component id="8d2b0" class="javax.swing.JButton" binding="btnCancelar">
            <constraints/>
            <properties>
              <text value="Cancelar"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
